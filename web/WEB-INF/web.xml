<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>NotaFiscalSaidaWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ContasReceberWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>DevolucaoNotaFiscalSaidaWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ContasReceberBoletoRejeitadoWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ContasReceberInterfaceWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SaidaMercadoriaWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>NotaFiscalSaidaWebService</servlet-name>
        <url-pattern>/NotaFiscalSaidaWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContasReceberWebService</servlet-name>
        <url-pattern>/ContasReceberWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DevolucaoNotaFiscalSaidaWebService</servlet-name>
        <url-pattern>/DevolucaoNotaFiscalSaidaWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContasReceberBoletoRejeitadoWebService</servlet-name>
        <url-pattern>/ContasReceberBoletoRejeitadoWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContasReceberInterfaceWebService</servlet-name>
        <url-pattern>/ContasReceberInterfaceWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaidaMercadoriaWebService</servlet-name>
        <url-pattern>/SaidaMercadoriaWebService</url-pattern>
    </servlet-mapping>

</web-app>
