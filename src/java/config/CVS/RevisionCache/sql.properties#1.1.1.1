parcelanotafiscalsaidadao.pesquisarParcelaPaga = SELECT INV6.INSTLMNTID TITULOPARCELAMODELNUMEROPARCELA, INV6.DOCENTRY TITULOPARCELAMODELID, RCT2.APPLIEDSYS AS VALORAPLICADO, ORCT.DOCENTRY CONTAS_RECEBER_ID,CASE ISNULL(ORCT.BOEABS,'0')  WHEN '0' THEN ORCT.DOCDATE ELSE OBOE.DATA_PAGAMENTO END AS DATA_PAGAMENTO FROM DBO.OINV OINV WITH (NOLOCK) INNER JOIN DBO.INV6 INV6 WITH (NOLOCK) ON (OINV.DOCENTRY = INV6.DOCENTRY) INNER JOIN DBO.RCT2 RCT2 WITH (NOLOCK) ON (RCT2.DOCENTRY = INV6.DOCENTRY AND RCT2.INSTID = INV6.INSTLMNTID) INNER JOIN DBO.ORCT ORCT WITH (NOLOCK) ON (ORCT.DOCENTRY = RCT2.DOCNUM) LEFT OUTER JOIN (SELECT MAX(TRANDATE) AS DATA_PAGAMENTO ,OBOE.BOESTATUS, OBOE.BOETYPE, OBOE.BOEKEY FROM DBO.OBOE WITH(NOLOCK) INNER JOIN DBO.BOT1 WITH(NOLOCK) ON (OBOE.BOEKEY = BOT1.BOEABS) INNER JOIN DBO.OBOT WITH(NOLOCK) ON (BOT1.ABSENTRY = OBOT.ABSENTRY) GROUP BY OBOE.BOESTATUS, OBOE.BOETYPE,OBOE.BOEKEY) AS OBOE ON (OBOE.BOEKEY = ORCT.BOEABS) WHERE RCT2.INVTYPE = 13 AND ORCT.CANCELED = 'N' AND ((OBOE.BOESTATUS IN('P','D') AND OBOE.BOETYPE = 'I' AND ORCT.BOEABS IS NOT NULL ) OR (ORCT.BOEABS IS NULL)) AND INV6.INSTOTALSY <> 0 AND ISNULL(ORCT.CREATEDATE,'1') = ISNULL(?,ISNULL(ORCT.CREATEDATE,'1')) AND ISNULL(OINV.NUMATCARD,'0') = ISNULL(?, ISNULL(OINV.NUMATCARD,'0')) AND ISNULL(OINV.U_ORIGEM,0) = ISNULL(?, ISNULL(OINV.U_ORIGEM,0))
