#notafiscalsaidadao.obter = SELECT TOP 1 ID FROM (SELECT OINV.DOCENTRY AS ID FROM DBO.OINV WITH(NOLOCK) WHERE NOT EXISTS ( SELECT 'X' FROM ORIN WITH(NOLOCK) INNER JOIN RIN1 WITH(NOLOCK) ON (ORIN.DOCENTRY = RIN1.DOCENTRY) WHERE ORIN.NUMATCARD = ? AND ORIN.SEQCODE = 1 AND RIN1.BASEENTRY = OINV.DOCENTRY) AND OINV.NUMATCARD = ? AND OINV.U_ORIGEM = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.EASYCLASS_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.ASSINATURA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.ATOL_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.FOTOGRAFIA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.SVG_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.COBRANCA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.VENDAAVULSA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO =? AND ORIGEM_ID = ?) AS A
notafiscalsaidadao.obter = SELECT TOP 1 ID FROM (SELECT OINV.DOCENTRY AS ID FROM DBO.OINV WITH(NOLOCK) WHERE NOT EXISTS ( SELECT 'X' FROM ORIN WITH(NOLOCK) INNER JOIN RIN1 WITH(NOLOCK) ON (ORIN.DOCENTRY = RIN1.DOCENTRY) WHERE ORIN.NUMATCARD = ? AND ORIN.SEQCODE = 1 AND RIN1.BASEENTRY = OINV.DOCENTRY) AND OINV.NUMATCARD = ? AND OINV.U_ORIGEM = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.EASYCLASS_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.ASSINATURA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.ATOL_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.FOTOGRAFIA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.SVG_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.COBRANCA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO = ? AND ORIGEM_ID = ? UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.VENDAAVULSA_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO =? AND ORIGEM_ID = ?  UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.CLASSIFICADOS_CONTRATO_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO =? AND ORIGEM_ID = ?  UNION ALL SELECT ID FROM INTEGRACAOSISTEMALEGADO.DBO.CLASSIFICADOS_EXECUCAO_NFF_SAIDA WITH(NOLOCK) WHERE ID_EXTERNO =? AND ORIGEM_ID = ?) AS A
notafiscalsaidadao.pesquisarNotaFiscalSaidaTotalmenteAberta = SELECT DOCENTRY FROM OINV WITH(NOLOCK) WHERE OINV.DOCTOTALSY = (SELECT  SUM(VABERTO.VALORABERTO) FROM (SELECT INV6.DOCENTRY ID, CASE INV6.STATUS WHEN 'O' THEN (INV6.INSTOTALSY - (INV6.PAIDSYS - (SELECT ISNULL(SUM(CRECEBER_LINHA1.APPLIEDSYS),0) FROM DBO.ORCT CRECEBER1 WITH (NOLOCK) INNER JOIN DBO.RCT2 CRECEBER_LINHA1 WITH (NOLOCK) ON CRECEBER1.DOCENTRY = CRECEBER_LINHA1.DOCNUM  INNER JOIN DBO.OBOE OBOE WITH (NOLOCK) ON (OBOE.BOENUM = CRECEBER1.BOENUM) WHERE CRECEBER_LINHA1.INVTYPE = 13 AND OBOE.BOESTATUS ='G' AND OBOE.BOETYPE = 'I' AND CRECEBER_LINHA1.DOCENTRY = INV6.DOCENTRY AND CRECEBER_LINHA1.INSTID = INV6.INSTLMNTID )) ) WHEN 'C' THEN  T0.VALORBOLETO END AS VALORABERTO FROM DBO.INV6 INV6 WITH (NOLOCK) LEFT OUTER JOIN (SELECT RCT2.APPLIEDSYS AS VALORBOLETO, OBOE.BOENUM, RCT2.DOCENTRY , RCT2.INSTID,	OBOE.BOESTATUS STATUSOBOE,OBOE.BOETYPE ,RCT2.INVTYPE ,ORCT.BOESTATUS STATUSORCT, OBOE.BOEKEY FROM DBO.RCT2 RCT2 WITH (NOLOCK)  LEFT OUTER JOIN DBO.ORCT ORCT WITH (NOLOCK) ON (ORCT.DOCENTRY = RCT2.DOCNUM) LEFT OUTER JOIN DBO.OBOE OBOE WITH (NOLOCK) ON (OBOE.BOENUM = ORCT.BOENUM) WHERE RCT2.INVTYPE = 13 AND OBOE.BOESTATUS ='G' AND OBOE.BOETYPE = 'I' AND ORCT.BOESTATUS IS NOT NULL) AS T0 ON (T0.DOCENTRY = INV6.DOCENTRY AND T0.INSTID = INV6.INSTLMNTID) WHERE (INV6.STATUS = 'O' OR (INV6.STATUS = 'C' AND T0.BOENUM IS NOT NULL)) AND INV6.INSTOTALSY <> 0 ) AS VABERTO WHERE VABERTO.ID = OINV.DOCENTRY) AND ISNULL(OINV.U_ORIGEM,0) =ISNULL(?, ISNULL(OINV.U_ORIGEM,0)) AND ISNULL(OINV.NUMATCARD,'0') = ISNULL(?,ISNULL(OINV.NUMATCARD,'0'))

